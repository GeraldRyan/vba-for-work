'End With
Sub Macro3()
'
' Macro3 Macro
'

'
    Selection.Cut
    Range("E9").Select
    ActiveSheet.Paste
    Sheets("original").Select
    ActiveWindow.SmallScroll Down:=-30
    Sheets("original").Select
    ActiveWindow.SmallScroll Down:=-39
    Sheets("original (5)").Select
    Sheets("original (5)").Name = "original (5)"
    Range("A21").Select
    Sheets("original (5)").Select
    Sheets("original (5)").Copy Before:=Sheets(1)
    Range("F16").Select
End Sub
